{
  "files.associations": {
    "*.css": "tailwindcss",
    "dep5": "debian-copyright"
  },
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "npm.packageManager": "pnpm",
  "stylelint.packageManager": "pnpm",
  "eslint.packageManager": "pnpm",
  "prettier.configPath": ".prettierrc.json",
  "prettier.requireConfig": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "stylelint.reportDescriptionlessDisables": true,
  "stylelint.reportInvalidScopeDisables": true,
  "stylelint.reportNeedlessDisables": true,
  "laravel-pint.enable": true,
  "intelephense.format.enable": false,
  "intelephense.environment.phpVersion": "8.0",
  "intelephense.environment.shortOpenTag": false,
  "intelephense.phpdoc.returnVoid": false,
  "intelephense.rename.exclude": ["**/vendor/**"],
  "intelephense.references.exclude": ["**/vendor/**"],
  "intelephense.stubs": [
    "apache",
    "bcmath",
    "bz2",
    "calendar",
    "com_dotnet",
